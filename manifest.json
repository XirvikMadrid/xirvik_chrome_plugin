{
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "css": [
        "css/dialogs.css",
        "css/ui-lightness/jquery-ui.css",
        "css/jquery.fancybox.css"
      ],
      "js": [
        "config.js",
        "content.js",
        "util.js",
        "document_event.js",
        "vendor/jquery.js",
        "vendor/jquery-ui.js",
        "vendor/jquery.fancybox.js"
      ],
      "matches": [
        "\u003Call_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_ext_description__",
  "icons": {
    "128": "images/xirvik-128.png",
    "16": "images/xirvik-16.png",
    "32": "images/xirvik-32.png",
    "48": "images/xirvik-48.png",
    "64": "images/xirvik-64.png",
    "96": "images/xirvik-96.png"
  },
  "key": "",
  "manifest_version": 3,
  "minimum_chrome_version": "101.0.0.0",
  "name": "__MSG_ext_name__",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage",
    "notifications",
    "declarativeNetRequest"
  ],
  "short_name": "__MSG_ext_short_name__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.0",
  "web_accessible_resources": [
    {
      "resources": [
        "upload_options.html",
        "seedboxes.html",
        "config.js",
        "content.js",
        "util.js",
        "vendor/jquery.js",
        "vendor/jquery-ui.js",
        "vendor/jquery.fancybox.js",
        "css/*.css",
        "css/ui-lightness/images/*.png",
        "images/*.png",
        "images/*.gif"
      ],
      "matches": [
        "\u003Call_urls>"
      ]
    }
  ],
  "action": {
    "default_icon": "images/xirvik-16.png",
    "default_popup": "options.html",
    "default_title": "Xirvik"
  },
  "content_security_policy": {},
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}